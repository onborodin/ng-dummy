#!/bin/sh
#
# $Id$
#
# PROVIDE: wcm
# REQUIRE: LOGIN

. /etc/rc.subr

name="wcm"
rcvar="wcm_enable"

pidfile="@APP_RUNDIR@"/wcm.pid
command="@prefix@/sbin/${name}"
command_args="--daemon"
procname="@prefix@/sbin/${name}"
command_interpreter="@node@"

load_rc_config ${name}

: ${wcm_enable:="NO"}

run_rc_command "$1"
#EOF
