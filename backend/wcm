#!/bin/sh

NODE_PATH="/usr/local/etc/wcm"
NODE_PATH="$NODE_PATH:/usr/local/share/wcm"
NODE_PATH="$NODE_PATH:/usr/local/share/wcm/models"
NODE_PATH="$NODE_PATH:/usr/local/share/wcm/controllers"
NODE_PATH="$NODE_PATH:/usr/local/share/wcm/utils"
NODE_PATH="$NODE_PATH:/usr/local/share/wcm/plugins"
NODE_PATH="$NODE_PATH:/usr/local/share/wcm/routers"
export NODE_PATH

NODE_ENV=production
export NODE_ENV

cd /usr/local/share/wcm && /usr/local/bin/node /usr/local/share/wcm/exserv.js $@
#EOF


